{
	"$schema": "http://json.schemastore.org/template",
	"author": "Ronny Natsuo",
	"classifications": [
	  "Web",
	  "Blazor"
	],
	"name": "Blazor Component with code-behind and CSS file",
	"generatorVersions": "[1.0.0.0-*)",
