	},
	"groupIdentity": "Miqo.Templates.BlazorComponentWithCss",
	"identity": "Miqo.Templates.BlazorComponentWithCss",
	"shortName": "miqo-blazorcss",
	"sourceName": "ComponentName",
	"primaryOutputs": [
		{ "path": "ComponentName.razor" },
		{ "path": "ComponentName.razor.cs" },
		{ "path": "ComponentName.razor.css" }
	],
	"defaultName": "Component",
	"symbols": {
		"namespace": {
		  "description": "namespace for the generated component",
		  "replaces": "MyApp.Namespace",
		  "type": "parameter"
